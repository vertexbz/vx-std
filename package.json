{
  "name": "vx-std",
  "version": "0.2.9",
  "license": "MIT",
  "description": "VX JavaScript standard library",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=12.0",
    "npm": ">=6.0",
    "yarn": "^1.0"
  },
  "dependencies": {
    "atob": "^2.1.2",
    "btoa": "^1.2.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc",
    "prepare": "yarn build && yarn docs",
    "lint": "tslint --project tsconfig.json --format codeFrame",
    "test": "jest -c ./jest.config.js",
    "docs": "typedoc -p . --theme default --target 'es6' --excludeNotExported --exclude ./src/index.ts --excludePrivate --ignoreCompilerErrors --out docs src/"
  },
  "devDependencies": {
    "@types/atob": "^2.1.2",
    "@types/btoa": "^1.2.3",
    "@types/jest": "^25.1.1",
    "jest": "^25.1.0",
    "rimraf": "^3.0.1",
    "ts-jest": "^25.1.0",
    "tsc": "^1.20150623.0",
    "tslib": "^1.10.0",
    "tslint": "^6.0.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-etc": "^1.10.0",
    "typedoc": "^0.16.9",
    "typescript": "^3.7.5",
    "typescript-tslint-plugin": "^0.5.5"
  },
  "files": [
    "dist"
  ]
}
