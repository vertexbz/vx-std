{
  "name": "vx-std",
  "version": "0.1.14",
  "license": "MIT",
  "description": "VX JavaScript standard library",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=12.0",
    "npm": ">=6.0",
    "yarn": "^1.0"
  },
  "dependencies": {
    "atob": "^2.1.2",
    "btoa": "^1.2.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc",
    "prepare": "yarn build && yarn docs",
    "lint": "tslint --project tsconfig.json --format codeFrame",
    "test": "jest -c ./jest.config.js",
    "docs": "typedoc -p . --theme default --target 'es6' --excludeNotExported --exclude ./src/index.ts --excludePrivate --ignoreCompilerErrors --out docs src/"
  },
  "devDependencies": {
    "@types/atob": "^2.1.2",
    "@types/btoa": "^1.2.3",
    "@types/jest": "24.0.15",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tsc": "^1.20150623.0",
    "tslib": "1.9.3",
    "tslint": "5.12.0",
    "tslint-config-standard": "8.0.1",
    "tslint-etc": "1.2.12",
    "typedoc": "^0.14.2",
    "typescript": "3.2.2",
    "typescript-tslint-plugin": "0.2.1"
  },
  "files": [
    "dist"
  ]
}
